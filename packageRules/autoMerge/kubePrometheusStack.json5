{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "description": "Auto merge KPS minors and patches",
      "matchDatasources": ["helm"],
      "automerge": true,
      "matchUpdateTypes": ["minor", "patch"],
      "matchDepNames": ["kube-prometheus-stack"],
      "ignoreTests": false,
    },
  ],
}